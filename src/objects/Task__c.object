    <fields>
        <fullName>Points_Award__c</fullName>
        <externalId>false</externalId>
        <formula>IF(  CONTAINS(RecordType.Name , &quot;Habit&quot;) , 
    1, 
     IF( ISPICKVAL( Difficulty__c , &quot;Trivial&quot;) , 
         IF(CONTAINS(RecordType.Name, &quot;TO-DO&quot;), 
              1, 
              0.5), 
     IF(ISPICKVAL(Difficulty__c,&quot;Easy&quot;), 
         IF( CONTAINS(RecordType.Name,&quot;TO-DO&quot;), 
              2, 
              1), 
     IF(ISPICKVAL(Difficulty__c,&quot;Medium&quot;), 
           IF(CONTAINS(RecordType.Name,&quot;TO-DO&quot;), 
                  3, 
                  1.5) , 
     IF(ISPICKVAL(Difficulty__c,&quot;Hard&quot;), 
           IF(CONTAINS(RecordType.Name,&quot;TO-DO&quot;), 
                 5, 
                 2.5) , 
           0) ) ) ))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>Points Award calculation based on Difficulty</inlineHelpText>
        <label>Points Award</label>
        <precision>18</precision>
        <required>false</required>
        <scale>1</scale>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
